datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int     @id @default(autoincrement())
  name     String
  tagIds   String
  roleList String
  phone1   String
  phone2   String
  phone3   String
  metadata Json
}

model Product {
  id           Int     @id @default(autoincrement())
  name         String
  categoryIds  String
  address1     String
  address2     String
  attributes   Json
}
